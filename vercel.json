{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "starnet_converter.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/", "dest": "/public/index.html" }, 
      { "src": "/(.*)", "dest": "/public/$1" }, 
      { "src": "/convert", "dest": "server.js" } 
    ]
  }